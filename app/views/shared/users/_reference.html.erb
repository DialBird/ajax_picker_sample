<div id="modal" class="modal fade">
  <div class="modal-dialog modal-content" style="width:750px;">
    <div class="modal-header">
      <button class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <h5 class="modal-title">選択</h5>
    </div>
    <div class="modal-body">
      <%= render 'shared/panel' do %>
        <%= form_for Search::User.new, url: reference_users_search_path, method: :get, remote: true, html: { class: 'form-horizontal', role: 'form' } do |f| %>
          <%= render 'shared/users/search_form', f: f %>
        <% end %>
        <div class="list">
        </div>
      <% end %>
    </div>
    <div class="modal-footer">
      <%= button_tag '選択', class: 'btn btn-primary select', data: { dismiss: 'modal' } %>
    </div>
  </div>
</div>
